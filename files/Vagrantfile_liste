Vagrant.configure("2") do |config|
  config.vm.define "mysql" do |mysql|
    mysql.vm.box = "debian10.box"
    mysql.vm.hostname = "web01"
    mysql.vm.network "private_network", ip: "10.0.0.1"

  end

  config.vm.define "ftp" do |ftp|
    ftp.vm.box = "debian10.box"
    ftp.vm.hostname = "web02"
    ftp.vm.network "private_network", ip: "10.0.0.2"
  end

end
